{
    "name": "alibahari/auth-api",
    "description": "Simple PHP Auth API with JWT and Refresh Token",
    "type": "project",
   "require": {
    "php": ">=8.2",
    "firebase/php-jwt": "^7.0",
    "vlucas/phpdotenv": "^5.6",
        "auth0/auth0-php": "^8.18",
        "nyholm/psr7": "^1.8"
}
,
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "license": "proprietary",
    "require-dev": {
        "phpunit/phpunit": "^12.5"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
